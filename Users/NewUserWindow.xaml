<Controls:MetroWindow x:Class="Users.NewUserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:Controls="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
        xmlns:local="clr-namespace:Users"
        mc:Ignorable="d"
        Title="Добавить клиента" Height="150" Width="500" WindowStartupLocation="CenterScreen" ResizeMode="CanMinimize">
    <Grid>
        <StackPanel>
            <TextBox x:Name="textbox" Margin="100 35 100 0" Text="{Binding FIO}"/>
        </StackPanel>
        <Button Content="Добавить" IsDefault="true" Height="10" Width="100" Margin="100 60 100 0" Click="Button_Click_1"></Button>
    </Grid>
</Controls:MetroWindow>
